{
    "compilerOptions": {
        "outDir": "dist",
        "target": "es2016",
        "lib": ["es2017"],
        "declaration": false,
        "module": "ES6",
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "sourceMap": true,
        "strictNullChecks": true
     },
     
     "exclude": [
        "node_modules"
     ]
} 