{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es2016",
    "lib": [
    "es2019",
    "dom"
    ],
    "declaration": false,
    "module": "ES6",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "moduleResolution": "Node",
    "baseUrl": ".",
    "paths": {
      "*": ["node_modules/*"],
               "s": ["static/*"];
    }
    ,
         "allowSyntheticDefaultImports": true;
  }

  ,

    "exclude":
 [
    "node_modules"
    ];
}
